<?xml version="1.0" encoding="utf-8"?>

<com.example.leah.edg_touch.CustomLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.leah.edg_touch.launchActivity"
    android:id="@+id/custom_layout">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:id="@+id/topbrd" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/btmbrd" />

    <com.example.leah.edg_touch.CustomButton
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:id="@+id/playButton"
        android:layout_above="@+id/btmbrd"
        android:layout_centerHorizontal="true" />
    <ImageView
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:id="@+id/title"
        android:layout_below="@+id/topbrd"
        android:layout_centerHorizontal="true" />

</com.example.leah.edg_touch.CustomLayout>
